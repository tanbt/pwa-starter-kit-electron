{
  "name": "PwaStarterKit",
  "description": "Designer PWA using Polymer",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "installer": "node installer.js",
    "serve": "gulp serve",
    "build": "tsc && npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/lit-element": "^0.6.2",
    "@polymer/polymer": "^3.1.0",
    "@vaadin/vaadin-button": "^2.1.0",
    "@vaadin/vaadin-designer-license-dialog": "git@github.com:vaadin/vaadin-designer-license-dialog.git#master",
    "@vaadin/vaadin-designer-outline": "git@github.com:vaadin/vaadin-designer-outline.git#update-polymer-version",
    "@vaadin/vaadin-designer-palette": "git@github.com:vaadin/vaadin-designer-palette.git#2.x",
    "@vaadin/vaadin-designer-paper": "git@github.com:vaadin/vaadin-designer-paper.git#master",
    "@vaadin/vaadin-designer-properties": "git@github.com:vaadin/vaadin-designer-properties.git#update-polymer-version",
    "@vaadin/vaadin-designer-viewport": "git@github.com:vaadin/vaadin-designer-viewport.git#master",
    "@vaadin/vaadin-dnd": "git@github.com:vaadin/vaadin-dnd.git#master",
    "@vaadin/vaadin-split-layout": "^4.1.0-beta2",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "electron": "^3.0.9",
    "electron-winstaller": "^2.7.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.7.6",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.2.0",
    "puppeteer": "^1.5.0",
    "typescript": "^3.1.3",
    "wct-browser-legacy": "^1.0.1"
  }
}
