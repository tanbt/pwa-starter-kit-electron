{
  "entrypoint": "index.html",
  "shell": "src/components/my-app.js",
  "sources": [
    "images/**/*",
    "node_modules/**/icons/**/*",
    "node_modules/**/images/**/*",
    "node_modules/**/fonts/**/*",
    "node_modules/**/snippets/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "push-manifest.json"
  ],
  "builds": [
    {
      "name": "es6-bundled",
      "basePath": false,
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": false,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
